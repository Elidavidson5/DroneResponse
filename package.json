{
  "name": "hiloscope",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.7.9",
    "cra-template": "1.2.0",
    "framer-motion": "^11.15.0",
    "leaflet": "^1.9.4",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "concurrently \"npm run start-react\" \"npm run start-backend\"",
    "start-react": "react-scripts start",
    "start-backend": "cd src/API && export FLASK_APP=backend.py && flask run --host=0.0.0.0 --port=4000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "most-date": "npm install && cd src/API && pip install -r requirements.txt"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.15",
    "concurrently": "^9.1.2",
    "tailwindcss": "^3.4.17"
  }
}
